{
  "name": "Auto-click Steam Lista de Descobrimento",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "activeTab",
    "*://store.steampowered.com/*",
    "*://*/*",
    "<all_urls>"
  ],
  "manifest_version": 2
}
